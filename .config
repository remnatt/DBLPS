--| 'config for private server'
^ 'main settings'
  state isAntiCheatActive = 'FALSE'
  state logActions = 'PROMPT'
  state portNum = '2025'
  state useOffsetsVIAbinfile = 'TRUE'
  state localPvP = 'TRUE'
  state useNewUI = 'PROMPT'
  state AllowProudBattlePvP = 'TRUE'
  state addSponsorBanner = 'TRUE'
  state useDiscordVerification = 'TRUE'
  state InitialGiftBoxCC = int'202025'
  state skipTutorial = 'TRUE'
>

--| 'More configs'
  ^ 'extra settings'
    state crystalDepositCD = 'One Week'
    state allowCustomAssets = 'TRUE'
    state japaneseBuildOut = 'TRUE ON COMPILE'
    state databaseModule = 'SQLite'
    state AddNewGroundBattle = 'FALSE' -- 'Might not be stable on the private server'
>
